{
  "name": "fna",
  "version": "0.2.12",
  "description": "finap360 native app",
  "keywords": [
    "react",
    "expo",
    "template",
    "typescript",
    "nativebase"
  ],
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "downloadExpoApp": "e2e/misc/downloadExpoApp.sh",
    "test": "jest",
    "test-e2e:ios": "detox test --configuration ios",
    "build-e2e:ios": "detox build --configuration ios",
    "test-e2e:android": "detox test --configuration android",
    "build-e2e:android": "detox build --configuration android"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@config-plugins/detox": "^1.2.0",
    "@hookform/resolvers": "^2.8.8",
    "@react-native-community/datetimepicker": "4.0.0",
    "axios": "^0.26.0",
    "date-fns": "^2.28.0",
    "expo": "^44.0.0",
    "expo-dev-client": "~0.8.4",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "native-base": "^3.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "7.17.5",
    "react-native": "0.64.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "react-query": "^3.34.15",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/generator": "^7.0.0-beta.47",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "babel-core": "^7.0.0-beta.47",
    "detox": "^19.5.3",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "jest-expo": "^44.0.0",
    "jest-fetch-mock": "^3.0.3",
    "miragejs": "^0.1.43",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "~4.0.0"
  },
  "author": "Felix Schneider",
  "private": true,
  "jest": {
    "verbose": true,
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "globals": {
      "window": {}
    },
    "automock": false,
    "testEnvironment": "jsdom"
  }
